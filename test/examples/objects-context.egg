do (
  def(x, object ( 
    "c", 0,
    "gc", ->( self.c ),
    "sc", ->(value, =(self.c, value)),
    "inc", ->(=(self.c, +(self.c, 1))),
    "inc2", ->(=(self["c"], +(element(self, "c"),1)))
  )),
  print(x),
  print(x.gc()), # 0
  x.sc(4),
  print(x["gc"]()), # 4
  x["inc"](),
  print(x.gc()), # 5
  x["inc2"](),
  print(x["gc"]()), # 6
)

